<h1>{{title}}</h1>
<p><a href="/logout">Logout</a></p>
<table class="table table-bordered">
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Website</th>
        <th>Action</th>
    </tr>
    </thead>
    <tbody>
    {{# each users }}
        <tr>
            <td>{{this.email}}</td>
            <td>{{this.username}}</td>
            <td>{{this.password}}</td>
            <td>
                <a href="/tickets/{{this._id}}" class="btn btn-success">View</a>
                <a href="/tickets/delete/{{this._id}}" class="btn btn-danger">Delete</a>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>